<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.api.test.dao.TestCaseDao">
    <select id="queryAll" resultType="com.api.test.entity.TestCaseEntity">
        select * from test_case limit 1
    </select>

    <update id="updateById" parameterType="com.api.test.entity.TestCaseEntity">
        update test_case set caseLevel="P1" where id =#{id}
    </update>


</mapper>
